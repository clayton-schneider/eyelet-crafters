{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "ignoreDeprecations": "5.0",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "types": ["astro/client"],
    "paths": {
      "@components/*": ["src/components/*"],
      "@img/*": ["src/img/*"],
      "@data/*": ["src/data/*"]
    }
  }
}
